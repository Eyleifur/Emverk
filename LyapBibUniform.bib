@InProceedings{pwql2012amrosino,
  Title                    = {Robust Stability of linear uncertain systems through Piecewise Quadratic {L}yapunov Functions defined over conical partitions},
  Author                   = {Ambrosino, R. and Garone, E.},
  Booktitle                = {Proceedings of the 51st IEEE Conference on Decision and Control},
  Year                     = {2012},

  Address                  = {Maui (HI), USA},
  Month                    = {Dec},
  Pages                    = {2872-2877},

  Abstract                 = {In this paper a class of Piecewise Quadratic Lyapunov Functions (PQLFs) for the analysis of the robust stability of linear systems subject to polytopic uncertainties is considered. These functions are obtained by partitioning the state space into polyhedric conical sets and by associating to each cone a quadratic form. This class of Lyapunov functions is not only a generalization of quadratic Lyapunov functions but it also (strictly) contains the class of polyhedral Lyapunov functions. The latter implication directly proves the universality of these functions for the robust stability problem and justifies the effort of finding a Lyapunov function belonging to this class. The technicalities for an effective use of this class of functions to test the robust stability of linear uncertain systems will be detailed. Numerical examples show that the proposed methodology allows one to obtain results that, compared with the actual state of the art, perform better under several viewpoints.},
  Doi                      = {10.1109/CDC.2012.6427016},
  ISSN                     = {0743-1546},
  Keywords                 = {Lyapunov methods;linear systems;robust control;set theory;state-space methods;uncertain systems;PQLF;generalized quadratic Lyapunov functions;linear uncertain system robust stability analysis;piecewise quadratic Lyapunov functions;polyhedral Lyapunov functions;polyhedric conical sets;polytopic uncertainties;quadratic formed cone partitioning;state space partitioning;Lyapunov methods;Robust stability;Robustness;Silicon;Stability analysis;Symmetric matrices;Uncertain systems},
  Owner                    = {DrSiggi},
  Timestamp                = {2014.08.01}
}

